    </head>
    <body class="text-center" >
      <div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
      <%= render "navigation/header" %>
        <main role="main" class="cover inner">
         <h1 class="cover-heading">Adhoc Web Services</h1>
         <p class="lead">Designing web services and products for you today!</p>
          <div class="form row">
            <div class="form-group col-auto m-auto" margin="auto">
              <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                <%= devise_error_messages! %>
                <div class="form-row">
                  <div class="col">
                    <input type="text" class="form-control mb-2" id="name" name="user[name]" placeholder="Jane Doe">
                  </div>
                </div>
                <div class="form-row">
                  <div class="col">
                    <input type="email" class="form-control mb-2" name="user[email]" id="email" placeholder="user@example.com">
                  </div>
                </div>
                <div class="form-row">
                  <div class="col">
                    <input type="password" class="form-control mb-2" name="user[password]" id="password" placeholder="Password (Must be 6 chars min)">
                </div>
                </div>
                <div class="form-row">
                  <div class="col">
                    <input type="password" class="form-control mb-2" name="user[password_confirmation]" id="password_confirmation" placeholder="******">
                </div>
                </div>
                <div class="form-row">
                  <div class="col">
                    <br><%= f.submit "Sign up" , :class =>"btn btn-lg btn-primary" %>
                  </div>
                </div>
              <% end %>
              <div class="row">
                <div class="col-auto mb-auto"><%= render "devise/shared/links" %></div>
              </div>
          </div>
        </main>
        <%= render "navigation/footer" %>
      </body>
